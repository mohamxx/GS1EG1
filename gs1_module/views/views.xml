<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="gs1_module.list">
      <field name="name">gs1_module list</field>
      <field name="model">gs1_module.gs1_module</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="gs1_module.action_window">
      <field name="name">gs1_module window</field>
      <field name="res_model">gs1_module.gs1_module</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="gs1_module.action_server">
      <field name="name">gs1_module server</field>
      <field name="model_id" ref="model_gs1_module_gs1_module"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="GS1" id="gs1_module.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="gs1_module.menu_1" parent="gs1_module.menu_root"/>
    <menuitem name="Menu 2" id="gs1_module.menu_2" parent="gs1_module.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="gs1_module.menu_1_list" parent="gs1_module.menu_1"
              action="gs1_module.action_window"/>
    <menuitem name="Server to list" id="gs1_module" parent="gs1_module.menu_2"
              action="gs1_module.action_server"/>

  </data>
</odoo>